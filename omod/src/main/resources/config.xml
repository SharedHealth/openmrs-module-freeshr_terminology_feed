    <advice>
        <point>org.openmrs.module.bdshrclient.SomeService</point>
        <class>org.openmrs.module.bdshrclient.advice.SomeAdvice</class>
    </advice>
-->
    <!-- Internationalization -->
    <!-- All message codes should start with ${project.parent.artifactId}. -->
